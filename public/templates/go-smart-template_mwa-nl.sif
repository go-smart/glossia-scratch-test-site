  In Tissue = Real 0.0
  Electric Conductivity = $CONSTANT_MW_ELECTRIC_CONDUCTIVITY_AIR:float
  Relative Permeability = $CONSTANT_MW_RELATIVE_PERMEABILITY_AIR:float
  Relative Permittivity = $CONSTANT_MW_RELATIVE_PERMITTIVITY_AIR:float
  Heat Capacity = $CONSTANT_SPECIFIC_HEAT_CAPACITY_TISSUE:float
  Heat Conductivity = $CONSTANT_THERMAL_CONDUCTIVITY_TISSUE:float
  Density = $CONSTANT_DENSITY_TISSUE:float
