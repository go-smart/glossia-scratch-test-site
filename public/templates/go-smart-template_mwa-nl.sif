!====================================================================

!====================================================================
Material 4
!====================================================================
  Name = "Catheter"

  In Tissue = Real 0.0
  Electric Conductivity = $CONSTANT_MW_ELECTRIC_CONDUCTIVITY_CATHETER:float
  Relative Permeability = $CONSTANT_MW_RELATIVE_PERMEABILITY_CATHETER:float
  Relative Permittivity = $CONSTANT_MW_RELATIVE_PERMITTIVITY_CATHETER:float
  Heat Capacity = $CONSTANT_SPECIFIC_HEAT_CAPACITY_TISSUE:float
  Heat Conductivity = $CONSTANT_THERMAL_CONDUCTIVITY_TISSUE:float
  Density = $CONSTANT_DENSITY_TISSUE:float
!====================================================================
End
!====================================================================

!====================================================================
Material 5 ! Tumour
!====================================================================
    Body Temperature = $CONSTANT_BODY_TEMPERATURE:float
    Heat Conductivity = $CONSTANT_THERMAL_CONDUCTIVITY_TUMOUR:float
    Heat Capacity = $CONSTANT_SPECIFIC_HEAT_CAPACITY_TUMOUR:float
    Density = $CONSTANT_DENSITY_TUMOUR:float

    In Tissue = Real 1.0
    Electric Conductivity = Equals MyElectricConductivity
    Relative Permeability = $CONSTANT_MW_RELATIVE_PERMEABILITY_TUMOUR:float
    Relative Permittivity = Equals MyRelativePermittivity

    Vapourization Cut Off = $CONSTANT_VAPOURIZATION_CUT_OFF_TUMOUR
    !Vapourization Minimum Deposition = $CONSTANT_VAPOURIZATION_MINIMUM_DEPOSITION_TUMOUR
    !Vapourization Gradient = $CONSTANT_VAPOURIZATION_GRADIENT_TUMOUR
    !Coagulation Cut Off = $CONSTANT_COAGULATION_CUT_OFF_TUMOUR
    !Coagulation Minimum Deposition = $CONSTANT_COAGULATION_MINIMUM_DEPOSITION_TUMOUR
    !Coagulation Gradient = $CONSTANT_COAGULATION_GRADIENT_TUMOUR
!====================================================================
End
!====================================================================

!====================================================================
Material 6 ! Needle
!====================================================================
    Body Temperature = $CONSTANT_BODY_TEMPERATURE
    Heat Conductivity = $CONSTANT_THERMAL_CONDUCTIVITY_TISSUE
    Heat Capacity = $CONSTANT_SPECIFIC_HEAT_CAPACITY_TISSUE
    Density = $CONSTANT_DENSITY_TISSUE
    In Tissue = Real 1.0 ! For stability

    Electric Conductivity = $CONSTANT_MW_ELECTRIC_CONDUCTIVITY_CATHETER
    Relative Permeability = $CONSTANT_MW_RELATIVE_PERMEABILITY_CATHETER
    Relative Permittivity = $CONSTANT_MW_RELATIVE_PERMITTIVITY_CATHETER

    !Vapourization Minimum Deposition = $CONSTANT_VAPOURIZATION_MINIMUM_DEPOSITION_TUMOUR
    !Vapourization Gradient = $CONSTANT_VAPOURIZATION_GRADIENT_TUMOUR
    !Coagulation Cut Off = $CONSTANT_COAGULATION_CUT_OFF_TUMOUR
    !Coagulation Minimum Deposition = $CONSTANT_COAGULATION_MINIMUM_DEPOSITION_TUMOUR
    !Coagulation Gradient = $CONSTANT_COAGULATION_GRADIENT_TUMOUR
!====================================================================
End
